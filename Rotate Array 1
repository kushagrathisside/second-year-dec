#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int T=0;
    cin>>T;
    while(T)
    {
        int s,n;
        cin>>s>>n;
        int a[n];
        for(int i=0;i<n;i++)
        {
            cin>>a[i];
        }
        for(int i=s;i<n;i++)
        {
            cout<<a[i]<<' ';
        }
        for(int i=0;i<s;i++)
        {
            cout<<a[i]<<' ';
        }
        cout<<endl;
        T--;
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
